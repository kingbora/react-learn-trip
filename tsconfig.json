{
    "compilerOptions":{
        "target": "es6",
        "module": "es6",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "suppressImplicitAnyIndexErrors": true,
        "jsx": "react", //忽略jsx，让jsx交给babel去处理
        "sourceMap": true,
        "experimentalDecorators": true
    },
    "compileOnSave": false,
    "exclude": [
        "node_modules"
    ]
}